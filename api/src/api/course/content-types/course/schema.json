{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "courses"
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "time": {
      "type": "float",
      "required": true
    },
    "title_short": {
      "type": "string"
    },
    "less": {
      "type": "component",
      "repeatable": true,
      "component": "lesson.less"
    }
  }
}
