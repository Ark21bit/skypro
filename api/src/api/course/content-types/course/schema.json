{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "courses"
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "time": {
      "type": "float"
    },
    "title_short": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "course"
    },
    "lesson": {
      "type": "component",
      "repeatable": true,
      "component": "lesson.lesson"
    }
  }
}
